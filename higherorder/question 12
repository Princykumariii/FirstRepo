let arr = [{  name :"Alice",marks:58},
    {name:"Bob",marks:85},
    {name:"Chalie",marks:92},
    {name:"David",marks:45}

]
let ans =arr.filter((el,i)=> el.marks>60);
console.log(ans)

let ans2 = arr.sort((el,i)=>el.marks>=45);
console.log(ans2)

let ans3 = arr.map((el,i)=>el.name);

console.log(ans3)